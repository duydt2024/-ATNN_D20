_1_sshd_config = main:cat.stdout : FILE_REGEX : ^@include\s+common-auth|^auth\s+required\s+pam_unix\.so|^@include\s+common-account
_1_auth_log = main:tail.stdout : FILE_REGEX : Accepted.*for ubuntu from 172\.18\.10\..*ssh2
_2_denied_access = main:tail.stdout : FILE_REGEX : sshd\[\d+\]: (pam_access\(sshd:account\): access denied for user.*from.*|error: PAM: User account has expired for \w+ from \d+\.\d+\.\d+\.\d+|fatal: monitor_read: unpermitted request \d+)
_2_accpt_access = main:tail.stdout : FILE_REGEX : Accepted.*for ubuntu from 172\.18\.10\..*ssh2
_3_authenticator = main:tail.stdout : FILE_REGEX : sshd\(pam_google_authenticator\)\[\d+\]: Accepted google_authenticator for ubuntu
_3_access_log = main:tail.stdout : FILE_REGEX : sshd\[\d+\]: Accepted.*for ubuntu from 172\.18\.10\..*ssh2
_4_pam_time = main:cat.stdout : FILE_REGEX : ^account\s+required\s+pam_time\.so
_4_denied_log = main:tail.stdout : FILE_REGEX : error: PAM: User account has expired for [a-zA-Z0-9._-]+ from [0-9]+\.[0-9]+\.[0-9]+\.[0-9]+
_5_denied_grp = main:tail.stdout : FILE_REGEX : sshd\[\d+\]:\s+pam_succeed_if\(sshd:account\):\s+requirement\s+"user\singroup\s+\w+"\s+not\s+met\s+by\s+user\s+"[\w\d]+"
_5_accpt_grp = main:tail.stdout : FILE_REGEX : sshd\[\d+\]:\s+pam_succeed_if\(sshd:account\):\s+requirement\s+"user\singroup\s+\w+"\s+was\s+met\s+by\s+user\s+"[\w\d]+"
