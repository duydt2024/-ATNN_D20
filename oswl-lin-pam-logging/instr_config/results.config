debug_conf = oswl-lin-pam-logging:cat.stdout : FILE_REGEX : ^auth\s+required\s+pam_debug.so
_debug_auth_suc = oswl-lin-pam-logging:tail.stdout : FILE_REGEX : Accepted password for.*from 172.16.10.3
_debug_auth_fail = oswl-lin-pam-logging:tail.stdout : FILE_REGEX : authentication failure.*logname.*uid.*rhost=172.16.10.3
compile_pam_custom = oswl-lin-pam-logging:gcc.stdin : CONTAINS : -fPIC -shared -o pam_custom.so pam_custom.c -lpam
pam_custom_conf = oswl-lin-pam-logging:cat.stdout : FILE_REGEX : ^auth\s+required\s+pam_custom.so
_custom_log_suc = oswl-lin-pam-logging:tail.stdout : FILE_REGEX : pam_custom.*login detected: ubuntu
_custom_log_fail = oswl-lin-pam-logging:tail.stdout : FILE_REGEX : pam_custom.*Unauthorized user
